<view class="content"><custom-header vue-id="417e1f62-1" title="{{pageName}}" showBack="{{true}}" headHeight="{{headerHeight}}" bind:__l="__l"></custom-header><view class="cont" style="{{'margin-top:'+(marginTop+'px')+';'}}"><view class="top_area" style="{{'top:'+(marginTop+tabMargin+'px')+';'}}"><view class="item"><view class="title flex flex-start" style="{{'height:'+(detailTitleHeight+'px')+';'+('line-height:'+(detailTitleHeight+'px')+';')}}"><image src="{{imgUrl+'/broker/ic_title.png'}}" mode="widthFix"></image><text>公司信息</text></view><view class="img_wrap"><view class="upload"><view class="top"><image src="{{info.business_license_image}}" mode="widthFix"></image></view><view class="bot">营业执照</view></view><view class="info_wrap"><view class="line flex flex-start" style="{{'height:'+(subTabHeight+'px')+';'+('line-height:'+(subTabHeight+'px')+';')}}"><view class="tit">公司名称：</view><view class="text">{{info.name}}</view></view><view class="line flex flex-start" style="{{'height:'+(subTabHeight+'px')+';'+('line-height:'+(subTabHeight+'px')+';')}}"><view class="tit">信用代码：</view><view class="text">{{info.unified_social_credit_code}}</view></view><view class="line flex flex-start" style="{{'height:'+(subTabHeight+'px')+';'+('line-height:'+(subTabHeight+'px')+';')}}"><view class="tit">法人姓名：</view><view class="text">{{info.legal_person_name}}</view></view><view class="line flex flex-start" style="{{'height:'+(subTabHeight+'px')+';'+('line-height:'+(subTabHeight+'px')+';')}}"><view class="tit">联系电话：</view><view class="text">{{info.legal_person_mobile}}</view></view><view class="line flex flex-start" style="{{'height:'+(subTabHeight+'px')+';'+('line-height:'+(subTabHeight+'px')+';')}}"><view class="tit">公司状态：</view><view class="text">{{info.status=="normal"?"正常":"已冻结"}}</view></view><view class="line flex flex-start" style="{{'height:'+(subTabHeight+'px')+';'+('line-height:'+(subTabHeight+'px')+';')}}"><view class="tit">办公地址：</view><view class="text">{{info.business_address}}</view></view></view></view></view></view></view></view>