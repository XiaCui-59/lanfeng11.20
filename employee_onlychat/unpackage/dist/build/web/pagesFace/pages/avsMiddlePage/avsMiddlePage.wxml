<scroll-view scroll-y="true" class="announcements">
    <view class="announcements-title">确认本人操作</view>
    <view class="mini-tip">意愿认证需采取你的人脸信息已核实身份</view>
    <view class="scan-picture">
      <image src="data:image/png;base64," mode="widthFix"/>
    </view>
    <view class="small-title">
      你需要：
    </view>
    <view class="icon-wrap">
      <view class="icon-item">
        <image src="data:image/png;base64," mode="widthFix"/>
        <view class="icon-text">正对屏幕</view>
      </view>
      <view class="icon-item">
        <image src="data:image/png;base64," mode="widthFix"/>
        <view class="icon-text">调高音量</view>
      </view>
      <view class="icon-item">
        <image src="data:image/png;base64," mode="widthFix"/>
        <view class="icon-text">环境安静</view>
      </view>
      <view class="icon-item">
        <image src="data:image/png;base64," mode="widthFix"/>
        <view class="icon-text">网络通畅</view>
      </view>
    </view>
    <view class="small-title">
      你不可以：
    </view>
    <view class="icon-wrap-2">
      <view class="icon-item">
        <image src="data:image/png;base64," mode="widthFix"/>
        <view class="icon-text">退出录制页面</view>
      </view>
      <view class="icon-item">
        <image src="data:image/png;base64," mode="widthFix"/>
        <view class="icon-text">录制屏幕</view>
      </view>
      <view class="icon-item">
        <image src="data:image/png;base64," mode="widthFix"/>
        <view class="icon-text">电话/会议通话</view>
      </view>
    </view>
  </scroll-view>
  <view class="confirm-bottom">
    <view wx:if="{{ !isDone }}" class="confirm-btn" bindtap="goFaceLive">开启认证</view>
    <view wx:else class="face-live-tips">结果查询中。。。</view>
  </view>

