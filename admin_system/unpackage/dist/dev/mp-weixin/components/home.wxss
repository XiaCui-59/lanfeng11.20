.board .data_wrap {
  margin-bottom: 16px;
}
.board .data_wrap .data_item {
  width: 24%;
  position: relative;
  font-size: 0;
}
.board .data_wrap .data_item > image {
  width: 100%;
}
.board .data_wrap .data_item:nth-child(1) {
  color: #216FF9;
}
.board .data_wrap .data_item:nth-child(2) {
  color: #27B470;
}
.board .data_wrap .data_item:nth-child(3) {
  color: #20A9DD;
}
.board .data_wrap .data_item:nth-child(4) {
  color: #FF6C1E;
}
.board .data_wrap .data_item .subtit {
  position: absolute;
  top: 16px;
  left: 32px;
  font-weight: 600;
  font-size: 16px;
}
.board .data_wrap .data_item .subtit image {
  margin-right: 5px;
}
.board .data_wrap .data_item .middle {
  position: absolute;
  top: 50%;
  left: 32px;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.board .data_wrap .data_item .middle .num {
  font-size: 44px;
  font-weight: 600;
  font-family: "Times New Roman";
}
.board .data_wrap .data_item .middle .period {
  color: #333;
  font-size: 15px;
  margin-left: 10px;
}
.board .list_wrap .list_item {
  height: 60px;
  line-height: 60px;
  background: #fff;
  border-radius: 8px;
  font-size: 15px;
  color: #333333;
  margin-bottom: 5px;
  cursor: pointer;
  transition: all .3s;
  font-weight: 600;
}
.board .list_wrap .list_item:hover {
  box-shadow: 0 2px 5px 2px #ddd;
  color: #20A9DD;
}
.board .list_wrap .list_item .left image {
  height: 60px;
  margin-right: 20px;
}
.board .list_wrap .list_item .right {
  padding-right: 20px;
}
.board .list_wrap .list_item .right image {
  margin-left: 70px;
}
.board .box {
  padding: 0;
  height: auto;
  min-height: auto;
}
.board .box .tabs {
  height: 60px;
  line-height: 60px;
}
.board .box .tabs .tab {
  width: 150px;
  height: 60px;
  text-align: center;
  position: relative;
  z-index: 99;
  cursor: pointer;
}
.board .box .tabs .tab:hover {
  color: #216FFf;
}
.board .box .tabs .tab::before {
  content: "";
  width: 0px;
  height: 23%;
  border-right: 1px dashed #333;
  position: absolute;
  top: 50%;
  left: 0;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.board .box .tabs .tab:nth-child(1)::before {
  display: none;
}
.board .box .tabs .tab.active {
  color: #fff;
}
.board .box .tabs .tab.active + .tab::before {
  display: none;
}
.board .box .tabs .tab.active::after {
  content: "";
  width: 100%;
  height: 70%;
  position: absolute;
  top: 50%;
  left: 0px;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  z-index: -1;
}
.board .box .tabs .tab.active::before {
  display: none;
}
.board .box .base_info .line {
  margin: 15px 0;
}
.board .box .base_info .line .sta {
  margin-right: 15px;
}
.board .box .base_info .line .sta image {
  width: 20px;
  height: 20px;
  margin-left: 5px;
}
.board .box .waiting .tabs {
  margin: 15px 0;
}
.board .box .waiting .tabs .tab {
  margin-right: 15px;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  cursor: pointer;
}
.board .box .waiting .tabs .tab.active {
  color: #226FF9;
  font-weight: 600;
}
.board .box .waiting .tabs .tab:hover {
  color: #226FF9;
}
.board .box .waiting .tabs .tab .mark {
  display: inline-block;
  width: 6px;
  height: 6px;
  background: #226FF9;
  border-radius: 50%;
  margin-left: 5px;
}

