.in_box {
  width: 60%;
  margin: 30px auto 0 auto;
}
.in_box .left .bot {
  font-weight: 400;
  font-size: 15px;
  color: #333333;
  line-height: 30px;
}
.in_box .left .top .line {
  font-size: 18px;
  color: rgba(0, 0, 0, 0.95);
  margin-bottom: 15px;
}
.in_box .left .top .line:nth-child(2) .text {
  font-family: 'Times New Roman', Times, serif;
  font-weight: 800;
  font-size: 26px;
  color: #226FF9;
}
.in_box .left .top .line .text image {
  width: 12px;
  margin-right: 5px;
}
.in_box .right .right_bot {
  margin-top: 80px;
  align-items: start;
}
.in_box .right .right_bot .bot_right {
  width: 50%;
}
.in_box .right .right_bot .bot_right image {
  width: 100%;
}
.in_box .right .right_bot .bot_left {
  width: 35%;
}
.in_box .right .right_bot .bot_left .title {
  margin-top: 0;
}
.in_box .right .right_bot .bot_left .code_wrap {
  margin-top: 20px;
}
.in_box .right .right_bot .bot_left .code_wrap .code {
  text-align: center;
}
.in_box .right .right_bot .bot_left .code_wrap .code image {
  width: 100%;
}
.in_box .right .right_bot .bot_left .code_wrap .tips {
  padding: 0 10px;
  box-sizing: border-box;
  height: 40px;
  line-height: 40px;
  margin-top: 10px;
  background: #00C800;
  border-radius: 2px;
  font-size: 12px;
  color: #FFFFFF;
  text-align: center;
}
.in_box .right .right_bot .bot_left .method {
  text-align: center;
  padding: 10px 0px;
  border-radius: 4px;
  border: 2px solid #226FF9;
  box-sizing: border-box;
  position: relative;
}
.in_box .right .right_bot .bot_left .method::after {
  content: "";
  width: 20px;
  height: 20px;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  bottom: 0;
  right: 0;
  position: absolute;
}
.in_box .right .right_bot .bot_left .method image {
  width: 20px;
  margin-right: 5px;
  vertical-align: middle;
}

